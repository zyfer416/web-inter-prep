{
  "version": 2,
  "builds": [
    {
      "src": "wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "wsgi.py"
    }
  ],
  "functions": {
    "wsgi.py": {
      "maxDuration": 30
    }
  },
  "env": {
    "FLASK_ENV": "production",
    "PYTHONPATH": "."
  }
}
